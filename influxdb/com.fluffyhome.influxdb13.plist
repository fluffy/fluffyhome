<?xml version="1.0" encoding="UTF-8"?>
<!-- copy this to  /Library/LaunchDaemons -->
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>com.fluffyhome.influxdb13</string>
	<key>RunAtLoad</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
	  <string>/usr/local/bin/influxd13</string>
	  <string>-config</string>
	  <string>/etc/influxdb/influxdb13.conf</string>
	</array>

  <key>UserName</key>
  <string>_postgres</string>

</dict>
</plist>
